{
  "name": "container",
  "version": "1.2.0",
  "homepage": "./",
  "main": "src/index.ts",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "new-remote": "node scripts/createRemoteScript.js && prettier --write package.json && prettier --write ./src/remotes/**/*.ts && prettier --write ./src/remotes/**/*.tsx && prettier --write ./src/remotes/remotes-mf.ts && prettier --write ../remotes/**/src/index.ts && prettier --write ./src/remotes/remote_paths.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cy:run": "cypress run",
    "test": "npm run cy:run cypress/integration/**/*.spec.js",
    "remotes:analytics": "concurrently -n \"analytics,container\"  -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"cd ../remotes/Analytics && npm run start\" \"npm run start\"",
    "test_environment": "concurrently -n \"analytics,settings,container\"  -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgRed.bold,bgYellow.bold,bgCyan.bold,bgWhite.bold\" \"cd ../remotes/Analytics && npm run start\" \"cd ../remotes/Settings && npm run start\" \"npm run start\"",
    "remotes:settings": "concurrently -n \"settings,container\"  -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"cd ../remotes/Settings && npm run start\" \"npm run start\""
  },
  "dependencies": {
